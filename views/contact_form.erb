<h1 class="text-center">Add a new contact</h1>

<form action="/contacts" method="post">
  <div class="form-group">
    <label target="first_name">First Name</label>
    <input id="first_name" name="first_name" class="form-control">
    <label target="last_name">Last Name</label>
    <input id="last_name" name="last_name" class="form-control">
  </div>
  <button type="submit" class="btn btn-default">Submit!</button>
</form>
